<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EDEDED"
    tools:context=".OpsiAntar">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="-140dp"
        android:layout_marginTop="-50dp"
        android:layout_marginEnd="-140dp"
        android:background="@drawable/corner_opsi"
        android:elevation="10dp"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="match_parent"
            android:layout_height="300dp"
            android:paddingTop="50dp"
            app:srcCompat="@drawable/take_away_pana"
            tools:layout_editor_absoluteX="155dp" />
    </androidx.appcompat.widget.Toolbar>

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Mau diantar kemana nih?"
        android:textColor="#050505"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/Proses"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="40dp"
        android:background="@drawable/rounded_opsi"
        android:elevation="10dp"
        android:minHeight="?attr/actionBarSize"
        android:onClick="Lantai1New"
        android:padding="20dp"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10">


    </androidx.appcompat.widget.Toolbar>

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="15dp"
        android:translationZ="10dp"
        app:layout_constraintStart_toStartOf="@+id/Proses"
        app:layout_constraintTop_toTopOf="@+id/Proses"
        app:srcCompat="@drawable/school" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="25dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Khusus Area SMKN 2 Buduran"
        android:textColor="#050505"
        android:textSize="12sp"
        android:translationZ="10dp"
        app:layout_constraintEnd_toEndOf="@+id/Proses"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toTopOf="@+id/Proses" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="-5dp"
        android:fontFamily="@font/poppins_medium"
        android:text="Lantai Bawah"
        android:textSize="12sp"
        android:translationZ="10dp"
        app:layout_constraintEnd_toEndOf="@+id/Proses"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />


    <androidx.appcompat.widget.Toolbar
        android:id="@+id/Terima"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="40dp"
        android:background="@drawable/rounded_opsi"
        android:elevation="10dp"
        android:minHeight="?attr/actionBarSize"
        android:onClick="Lantai2New"
        android:padding="20dp"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Proses">


    </androidx.appcompat.widget.Toolbar>

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="15dp"
        android:translationZ="10dp"
        app:layout_constraintStart_toStartOf="@+id/Terima"
        app:layout_constraintTop_toTopOf="@+id/Terima"
        app:srcCompat="@drawable/school" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="25dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Khusus Area SMKN 2 Buduran"
        android:textColor="#050505"
        android:textSize="12sp"
        android:translationZ="10dp"
        app:layout_constraintEnd_toEndOf="@+id/Terima"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView20"
        app:layout_constraintTop_toTopOf="@+id/Terima" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="-5dp"
        android:fontFamily="@font/poppins_medium"
        android:text="Lantai Atas"
        android:textSize="12sp"
        android:translationZ="10dp"
        app:layout_constraintEnd_toEndOf="@+id/Terima"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView20"
        app:layout_constraintTop_toBottomOf="@+id/textView20" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/Selesai"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="40dp"
        android:background="@drawable/rounded_opsi"
        android:elevation="10dp"
        android:minHeight="?attr/actionBarSize"
        android:onClick="LuarSekolah"
        android:padding="20dp"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Terima">


    </androidx.appcompat.widget.Toolbar>

    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="15dp"
        android:translationZ="10dp"
        app:layout_constraintStart_toStartOf="@+id/Selesai"
        app:layout_constraintTop_toTopOf="@+id/Selesai"
        app:srcCompat="@drawable/map" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="25dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Area Luar SMKN 2 Buduran"
        android:textColor="#050505"
        android:textSize="12sp"
        android:translationZ="10dp"
        app:layout_constraintEnd_toEndOf="@+id/Selesai"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toTopOf="@+id/Selesai" />

    <TextView
        android:id="@+id/textView88"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="-5dp"
        android:fontFamily="@font/poppins_medium"
        android:text="Isikan Alamat Anda"
        android:textSize="12sp"
        android:translationZ="10dp"
        app:layout_constraintEnd_toEndOf="@+id/Selesai"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView22"
        app:layout_constraintTop_toBottomOf="@+id/textView22" />


    <androidx.appcompat.widget.Toolbar
        android:id="@+id/dikoperasi"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="40dp"
        android:background="@drawable/rounded_opsi"
        android:elevation="10dp"
        android:minHeight="?attr/actionBarSize"
        android:onClick="ambildikoperasi"
        android:padding="20dp"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Selesai">


    </androidx.appcompat.widget.Toolbar>


    <ImageView
        android:id="@+id/imageView90"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="15dp"
        android:translationZ="10dp"
        app:layout_constraintStart_toStartOf="@+id/dikoperasi"
        app:layout_constraintTop_toTopOf="@+id/dikoperasi"
        app:srcCompat="@drawable/pickup" />

    <TextView
        android:id="@+id/textView90"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="25dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Ambil Barang Dikoperasi"
        android:textColor="#050505"
        android:textSize="12sp"
        android:translationZ="10dp"
        app:layout_constraintEnd_toEndOf="@+id/dikoperasi"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView90"
        app:layout_constraintTop_toTopOf="@+id/dikoperasi" />

    <TextView
        android:id="@+id/textView91"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="-5dp"
        android:fontFamily="@font/poppins_medium"
        android:text="Bayar DIkoperasi"
        android:textSize="12sp"
        android:translationZ="10dp"
        app:layout_constraintEnd_toEndOf="@+id/dikoperasi"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView90"
        app:layout_constraintTop_toBottomOf="@+id/textView90" />


</androidx.constraintlayout.widget.ConstraintLayout>